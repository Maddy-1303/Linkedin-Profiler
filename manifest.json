{
    "manifest_version": 3,
    "name": "LinkedIn Profile Saver",
    "version": "1.0",
    "description": "Save LinkedIn Profiles",
    "permissions": ["tabs"],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["content_script.js"],
        "run_at": "document_end"
      }
    ]
  }
  